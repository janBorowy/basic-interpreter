int main () {
    int a = 2;
    int b = 3;
    printIsEven(a, "a");
    printIsEven(b, "b");
    return 0;
}

void printIsEven(int i, string name) {
    if (i % 2 == 0) {
        println(name + " variable's value is even");
    } else {
        println(name + " variable's value is uneven");
    }
}